	pmid = {30043844},
	keywords = {Brazil, Cities, Drug Therapy, Geographic Information Systems, Health Services Accessibility, Health Services Needs and Demand, Humans, National Health Programs, Neoplasms, Programming, Linear, Residence Characteristics, Time Factors},
	pages = {e00055017},
}

@article{chuvieco_integration_1993,
	title = {Integration of linear programming and {GIS} for land-use modelling},
