	pmid = {25712202},
	keywords = {Adult, Aged, Female, Follow-Up Studies, Humans, Middle Aged, Young Adult, Adolescent, Male, Neoplasms, Time Factors, Patient Acceptance of Health Care, Survivors, Internet, Patient Education as Topic, Cancer communication, Cancer information, Cancer survivors, Health information, Information seeking, Information Seeking Behavior},
	pages = {348--357},
