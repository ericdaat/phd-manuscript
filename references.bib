	pages = {14490--14504},
	file = {Ni et al. - 2015 - An Enhanced Variable Two-Step Floating Catchment A.pdf:/Users/eric/Zotero/storage/MHWUKQNR/Ni et al. - 2015 - An Enhanced Variable Two-Step Floating Catchment A.pdf:application/pdf},
