	keywords = {Aged, Aged, 80 and over, Breast Neoplasms, Female, Follow-Up Studies, Humans, Male, Registries, Neoplasm Staging, United States, Physicians, Prospective Studies, Lymph Node Excision, Lymphatic Metastasis, Specialization, Sentinel Lymph Node Biopsy, Specialties, Surgical, Axilla, Clinical Competence},
	pages = {185--192},
