	pages = {102406},
	file = {Zhao et al. - 2020 - Unequable spatial accessibility to hospitals in de.pdf:/Users/eric/Zotero/storage/BGXZCHLU/Zhao et al. - 2020 - Unequable spatial accessibility to hospitals in de.pdf:application/pdf},
