	keywords = {Humans, Medical Oncology, Health Services Accessibility, Rural Health Services, Quality Improvement, Rural Population, Models, Organizational, Queensland, rural, telemedicine, Telemedicine, health service models, new models and frameworks, Organizational Innovation, telehealth},
	pages = {156--159},
	file = {Sabesan et al. - 2014 - Timely access to specialist medical oncology servi.pdf:/Users/eric/Zotero/storage/G7I8IVIV/Sabesan et al. - 2014 - Timely access to specialist medical oncology servi.pdf:application/pdf},
