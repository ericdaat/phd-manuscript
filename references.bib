	keywords = {Aged, Canada, Cancer Care Facilities, Carbon Footprint, Carbon footprint, Cross-Sectional Studies, Female, Head and neck neoplasms, Humans, Male, Middle Aged, Outreach, Surgicenters, Travel},
	pages = {26},
}

