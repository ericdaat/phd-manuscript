	pages = {75--174},
	file = {arXiv.org Snapshot:/Users/eric/Zotero/storage/E8YE72DY/0906.html:text/html;Submitted Version:/Users/eric/Zotero/storage/4XTSV562/Fortunato - 2010 - Community detection in graphs.pdf:application/pdf},
